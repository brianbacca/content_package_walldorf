{
	"_version": "1.15.0",
	"sap.app": {
		"id": "wall.CardBanner",
		"type": "card",
		"title": "Card Banner",
		"subTitle": "Card Banner",
		"applicationVersion": {
			"version": "1.0.8"
		}
	},
	"sap.ui": {
		"technology": "UI5",
        "deviceTypes": {"desktop": true, "phone": true, "tablet": true},
		"icons": {
			"icon": "sap-icon://technical-object"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "wall.CardBanner.View",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.38",
			"libs": {
				"sap.m": {}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"banners": {
				"type": "sap.ui.model.json.JSONModel"
			}
		}
	},
	"sap.card": {
		"type": "Component",
        "designtime": "dt/configuration"
	},
    "sap.platform.mobilecards": {
        "compatible": false
    }
}